{
  "name": "shopping_list",
  "version": "1.0.0",
  "description": "Application made using MERN Stack and includes user authentication",
  "main": "server.js",
  "scripts": {
    "client install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sleepysleep/Shopping_List.git"
  },
  "author": "Ashish Chourasia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sleepysleep/Shopping_List/issues"
  },
  "homepage": "https://github.com/sleepysleep/Shopping_List#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "config": "^3.3.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.0",
    "morgan": "^1.10.0"
  }
}
